<UITextEdit templateName="textedit" maxlength="16" changesound="keypress" errorsound="">
   <UIAspect>
      <default image="ui/shared/box.tga">
         <BFRect x="0" y="0" w="200" h="30"/>
      </default>
      <standard>
         <normal image="ui/shared/box.tga">
            <BFColor r="255" g="255" b="0" a="0"/>
         </normal>
         <highlighted image="ui/shared/box.tga">
            <BFColor r="255" g="255" b="0" a="255"/>
         </highlighted>
         <activated image="ui/shared/box.tga">
            <BFColor r="255" g="255" b="0" a="255"/>
         </activated>
      </standard>
      <alternate>
         <normal image="ui/shared/box.tga">
            <BFColor r="255" g="255" b="0" a="255"/>
         </normal>
         <highlighted image="ui/shared/box.tga">
            <BFColor r="255" g="255" b="0" a="255"/>
         </highlighted>
      </alternate>
   </UIAspect>
   <cursor>
      <UIStatic>
         <UIRegion x="0" y="0" w="3" h="18"/>
         <UIAspect>
            <default image="ui/shared/whiterec.tga">
               <BFColor r="0" g="0" b="0" a="255"/>
            </default>
            <BFColor r="125" g="125" b="125" a="200"/>
         </UIAspect>
      </UIStatic>
   </cursor>
</UITextEdit>
