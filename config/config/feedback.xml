		<guestthoughts:Bathroom threshold="20" priority="-1" interval="10" message="zoomessages:GuestsBathroom" />
		<guestthoughts:DessertBad threshold="20" priority="-1" interval="10" message="zoomessages:GuestsNeedDessert" />
