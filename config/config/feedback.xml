	<globalDisplay list="feedbackMessageList" last="feedbackLastMessage" count="25">
		<template priority="-1" file="ui/layout/messagebad.xml" />
		<template priority="0" file="ui/layout/messagenormal.xml" />
		<template priority="1" file="ui/layout/messagegood.xml" />
