		<animalthoughts:HungryAnimal threshold="10" priority="-1" interval="15" message="zoomessages:AnimalsHungry" />
		<animalthoughts:ThirstyAnimal threshold="10" priority="-1" interval="15" message="zoomessages:AnimalsThirsty" />
		<animalthoughts:EscapedAnimal threshold="1" priority="-1" interval="15" message="zoomessages:EscapedAnimal" />
