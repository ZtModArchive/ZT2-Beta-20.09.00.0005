		<guestthoughts:AmusementBad threshold="20" priority="-1" interval="10" message="zoomessages:GuestAmusementBad" />
		<guestthoughts:AnimalEntertaining threshold="40" priority="1" interval="10" message="zoomessages:GuestsEntertained" />
		<guestthoughts:AnimalAmusing threshold="40" priority="1" interval="10" message="zoomessages:GuestsAmused" />
