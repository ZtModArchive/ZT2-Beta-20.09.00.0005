		<guestthoughts:TrashFull threshold="20" priority="-1" interval="15" message="zoomessages:GuestsTrashFull" />
		<guestthoughts:EatStanding threshold="10" priority="-1" interval="15" message="zoomessages:GuestsStandingEat" />
